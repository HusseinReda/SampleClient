<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.imenu.sampleclient2">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/burger_icon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".controller.HttpGetRequestActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".controller.MealsGetRequestActivity"></activity>
        <activity
            android:name=".controller.TableNumbersActivity"
            android:label="@string/title_activity_table_numbers">
        </activity>
        <activity
            android:name=".controller.ChefActivity"
            android:label="@string/title_activity_chef"
           ></activity>
    </application>

</manifest>
